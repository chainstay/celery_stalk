{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "web",
      "image": "248213449538.dkr.ecr.us-east-1.amazonaws.com/celery_stalk:latest",
      "essential": true,
      "environment": [
        {
          "name": "POSTGRES_PASSWORD",
          "value": "local"
        }
      ],
      "memoryReservation": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000
        }
      ],
      "links": [
        "db"
      ]
    },
    {
      "name": "worker",
      "image": "248213449538.dkr.ecr.us-east-1.amazonaws.com/celery_stalk:latest",
      "essential": true,
      "memoryReservation": 128,
      "environment": [
        {
          "name": "RUN_AS_WORKER",
          "value": "TRUE"
        }
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "db",
      "image": "postgres:latest",
      "memoryReservation": 128,
      "essential": true
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "memoryReservation": 128,
      "essential": true
    }
  ]
}